generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointments {
  id          Int     @id @default(autoincrement())
  name        String
  hour        String
  service     String
  date        String
  barber      String
  isActive    Boolean
  wasCanceled Boolean
  isConcluded Boolean
}
